<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:alpha="0.5"
            android:background="@drawable/night1"
            android:orientation="horizontal"></LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:alpha="0.5"
            android:background="@drawable/darksky2"
            android:orientation="horizontal"></LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                android:orientation="horizontal">

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="6"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:fontFamily="sans-serif-black"
                    android:gravity="center"
                    android:text="전화번호부"
                    android:textColor="#E4DFDF"
                    android:textSize="48sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                android:orientation="horizontal">

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="15dp"
                android:layout_weight="5"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvName1"
                    android:layout_width="52dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="최민수 :"
                    android:textColor="#F6F1F1"
                    android:textSize="24sp" />

                <TextView
                    android:id="@+id/tvRename"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="010-1234-5678"
                    android:textColor="#F6F1F1"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/btnTel1"
                    android:layout_width="90dp"
                    android:layout_height="60dp"
                    android:background="@drawable/call3"
                    tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="15dp"
                android:layout_weight="5"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvName2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="공유 :"
                    android:textColor="#F6F1F1"
                    android:textSize="24sp" />

                <TextView
                    android:id="@+id/tvReage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="010-1234-1234"
                    android:textColor="#F6F1F1"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/btnTel2"
                    android:layout_width="90dp"
                    android:layout_height="60dp"
                    android:background="@drawable/call3"
                    tools:ignore="SpeakableTextPresentCheck" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="15dp"
                android:layout_weight="5"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvName3"
                    android:layout_width="58dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="송혜교 :"
                    android:textColor="#F6F1F1"
                    android:textSize="24sp" />

                <TextView
                    android:id="@+id/tvRetell"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="010-1111-2222"
                    android:textColor="#F6F1F1"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/btnTel3"
                    android:layout_width="90dp"
                    android:layout_height="60dp"
                    android:background="@drawable/call3"
                    tools:ignore="SpeakableTextPresentCheck" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="15dp"
                android:layout_weight="5"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvName4"
                    android:layout_width="54dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="유오성 :"
                    android:textColor="#F6F1F1"
                    android:textSize="24sp" />

                <TextView
                    android:id="@+id/tvReaddr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="010-3333-4444"
                    android:textColor="#F6F1F1"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/btnTel4"
                    android:layout_width="90dp"
                    android:layout_height="60dp"
                    android:background="@drawable/call3"
                    tools:ignore="SpeakableTextPresentCheck" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="15dp"
                android:layout_weight="5"
                android:gravity="center"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvName5"
                    android:layout_width="52dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="이동욱 :"
                    android:textColor="#F6F1F1"
                    android:textSize="24sp" />

                <TextView
                    android:id="@+id/tvPhone5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="010-2222-3333"
                    android:textColor="#F6F1F1"
                    android:textSize="24sp" />

                <Button
                    android:id="@+id/btnTel5"
                    android:layout_width="90dp"
                    android:layout_height="60dp"
                    android:background="@drawable/call3"
                    tools:ignore="SpeakableTextPresentCheck" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5"
                android:orientation="horizontal">

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5"
                android:orientation="horizontal" />
        </LinearLayout>

    </FrameLayout>

</LinearLayout>